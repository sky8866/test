<?xml version="1.0" encoding="UTF-8"?>
<decorators defaultdir="/WEB-INF/layouts/">
	<excludes>
		<pattern>/static/*</pattern>
		<pattern>/css/*</pattern>
		<pattern>/images/*</pattern>
		<pattern>/jsp/*</pattern>
		<pattern>/help/*</pattern>
		<pattern>/attached/*</pattern> 
		<pattern>/system/ajax/*</pattern>

	    <pattern>/web/home</pattern> 
	     <pattern>/admin/ajax/*</pattern>
	    <pattern>/phone/help/*</pattern>
	 
	</excludes>
	 
     
	
	<decorator name="default" page="default.jsp">
		 
		<pattern>/*</pattern>
			
	</decorator>
 
	 
	 
	 	<decorator name="default_admin" page="default_admin.jsp">
		 
		<pattern>/admin/*</pattern>
			
	</decorator>
	 
</decorators>
